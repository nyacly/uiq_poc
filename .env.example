# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/uiq_community_platform
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=password
PGDATABASE=uiq_community_platform

# Authentication & Session
NEXTAUTH_URL=http://localhost:5000
NEXTAUTH_SECRET=your-secret-key-here
SESSION_SECRET=your-session-secret-here

# Stripe Payment Integration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Twilio SMS Integration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Email Configuration (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Cloud Storage (Optional)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Analytics & Monitoring (Optional)
GOOGLE_ANALYTICS_ID=GA-XXXXX-X
SENTRY_DSN=your_sentry_dsn
UPTIME_ROBOT_API_KEY=your_uptime_robot_api_key

# Feature Flags (Optional)
ENABLE_REGISTRATION=true
ENABLE_PAYMENTS=true
ENABLE_SMS_NOTIFICATIONS=true

# Development
NODE_ENV=development
NEXT_PUBLIC_SITE_URL=http://localhost:5000
REPL_ID=your-repl-id-here
REPLIT_DOMAIN=your-app.replit.dev